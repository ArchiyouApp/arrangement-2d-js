{
  "compilerOptions": {
    "alwaysStrict": true,
    "noEmitOnError": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "removeComments": true,
    "strictNullChecks":true,
    "strict": true,
    "allowJs": true,
    "target": "esnext",
    "module": "esnext",
    "outDir": "build",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "allowSyntheticDefaultImports": true,

  }
}